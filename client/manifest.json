{
    "manifest_version": 2,
    
    "name": "commentary",
    "description": "This extension shows comments for a webpage",
    "version": "1.0",
    
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "permissions": [
	"activeTab",
	"https://ajax.googleapis.com/",
	"tabs", "https://docs.google.com/feeds/*"
    ]
    
}
